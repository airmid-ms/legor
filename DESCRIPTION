Package: legor
Type: Package
Title: Tools for Analyzing LEGO Data
Version: 0.1.0
Authors@R: c(person("Megan", "Healy", email="megan.healy.2022@mumail.ie", role= "aut"),
  person("Áirmid", "Murphy-Scott", email="airmid.murphyscott.2022@mumail.ie", role= c("aut", "cre")),
  person("Aaron", "Church", email="aaron.church.2021@mumail.ie", role= "aut"),
